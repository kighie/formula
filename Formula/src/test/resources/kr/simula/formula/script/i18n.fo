fndef 요율계산 (map 요율, text 나이, text 성별,  number 보험금){
	number result <- ROUND(요율["나이"][나이변수] * 요율["나이"][나이변수] * 보험금, 0);
	println( "** 요율 계산 나이:" & 나이 & ", 성별:" & 성별 & " -> "  & result );
	return result;
}

