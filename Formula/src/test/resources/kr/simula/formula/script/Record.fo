record sampleRecord <- R{
	number a : 2,
	number b : 1,
	int	   c ,
	String d ,
	number[]  nums : [1,2,3,4,5],
	map    map1 : { a:1, b:2 },
	map    map2 : {},
	map[]    maps : [{ a:1, b:2 }, {a:1,c:2}, {}],
	record recA : R{
		number a,
		text b
	},
	function fn : fndef(number a){
		System.out.println(a);
	},
	URL url
};

println("sampleRecord 1. ::" & sampleRecord);

record sampleRecord2 <- newRecord(sampleRecord);

