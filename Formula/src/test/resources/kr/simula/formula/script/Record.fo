record sampleRecord3 <- newRecord(sampleRecord);

println(sampleRecord3);

sampleRecord.recA.a <- 17;

println(sampleRecord.recA);


sampleRecord3.recA.a <- 19;

println(sampleRecord3.recA);


sampleRecord <- {
	a : 32,
	b : 7,
	c : 2,
	recA : { a:31 }
};

