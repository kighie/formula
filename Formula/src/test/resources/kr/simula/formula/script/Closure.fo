number localFunc(number argA, number argB) {
	number retVal;
	if(argA > argB) {
		println(argA & 'X' & argB);
		retVal <- CEILING(argA * argB, 3);
	} else {
		println(argA & '+' & argB); 
		LOG(argA + argB) -> retVal; // left to right assignment
	}
	return retVal;
} 

number fnA(number a){
	return a * 3;
}

number fnB(number a){
	return a * 7;
}

number fnC(number a){
